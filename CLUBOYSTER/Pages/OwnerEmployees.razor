@page "/ownerpanelemployee"
@using CLUBOYSTER.Data
@inject UserService usersService

@code {
        protected override async Task OnInitializedAsync()
    {
        if (usersService.GetUsers().Count == 0)
        {
            usersService.SetUsers();
        }
    }
}

<EmployeesTable users="usersService.GetUsers()"></EmployeesTable>
